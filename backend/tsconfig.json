{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["ES2022"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": false,
    "sourceMap": false,
    "noEmitOnError": false,
    "noEmit": true
  },
  "include": ["src/serverSimple.ts", "src/services/**/*", "src/types/**/*", "src/auth/**/*", "src/config/env.ts", "src/config/supabase.ts", "src/config/supabaseAdapter.ts"],
  "exclude": ["node_modules", "dist"]
}
